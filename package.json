{
  "name": "lyra-bot",
  "version": "1.1.0",
  "description": "Lyra trades bot",
  "license": "MIT",
  "engines": {
    "node": "16.x",
    "yarn": "1.x"
  },
  "scripts": {
    "start:lyrabot": "ts-node-dev --files --respawn src/app.ts",
    "start": "ts-node --transpile-only src/app.ts --env prod",
    "gen": "graphql-codegen --config codegen.yml",
    "lint:ts": "eslint --ext .js,.ts .",
    "lint:ts:fix": "yarn lint:ts --fix",
    "format": "prettier --write \"**/*.+(js|ts|json)\"",
    "deploycmds": "tsc && node ./deploy-commands.js",
    "generate-types": "yarn typechain --target ethers-v5 --out-dir src/contracts/typechain src/contracts/abis/*.json"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@discordjs/builders": "^0.15.0",
    "@discordjs/rest": "^0.5.0",
    "@lyrafinance/lyra-js": "^0.0.15",
    "@types/merge-images": "^1.2.1",
    "canvas": "^2.9.3",
    "coingecko-api": "^1.0.10",
    "cross-fetch": "^3.1.5",
    "dayjs": "^1.11.3",
    "discord-api-types": "^0.36.0",
    "discord.js": "^13.8.1",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.9",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "merge-images": "^2.0.0",
    "moment": "^2.29.3",
    "node-fetch": "3.2.6",
    "node-schedule": "^2.1.0",
    "react": "^18.2.0",
    "string-builder": "^0.1.8",
    "telegraf": "^4.8.5",
    "ts-node": "^10.8.2",
    "twitter-api-v2": "^1.12.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.4",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-codegen/typescript-resolvers": "2.6.6",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/coingecko-api": "^1.0.10",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.1",
    "@types/node-fetch": "2.6.2",
    "@types/node-schedule": "^2.1.0",
    "@types/react": "^18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typechain": "^8.0.0",
    "typescript": "^4.7.4",
    "yarn-upgrade-all": "^0.7.1"
  }
}
