{
  "name": "velo-bot",
  "version": "1.1.0",
  "description": "Velo bot",
  "license": "MIT",
  "engines": {
    "node": "16.x",
    "yarn": "1.x"
  },
  "scripts": {
    "start:bot": "ts-node-dev --files --respawn src/app.ts",
    "start": "ts-node --transpile-only src/app.ts --env prod",
    "lint:ts": "eslint --ext .js,.ts .",
    "lint:ts:fix": "yarn lint:ts --fix",
    "format": "prettier --write \"**/*.+(js|ts|json)\"",
    "generate-types": "yarn typechain --target ethers-v5 --out-dir src/contracts/typechain src/contracts/abis/*.json"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "@ethersproject/units": "^5.7.0",
    "axios": "^0.27.2",
    "canvas": "^2.9.3",
    "coingecko-api": "^1.0.10",
    "discord.js": "^14.3.0",
    "dotenv": "^16.0.0",
    "ethers": "^5.6.9",
    "lodash": "^4.17.21",
    "merge-images": "^2.0.0",
    "node-schedule": "^2.1.0",
    "react": "^18.2.0",
    "telegraf": "^4.8.5",
    "ts-node": "^10.8.2",
    "twitter-api-v2": "^1.12.3"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.0.0",
    "@types/coingecko-api": "^1.0.10",
    "@types/lodash": "^4.14.182",
    "@types/merge-images": "^1.2.1",
    "@types/node": "^18.0.1",
    "@types/node-schedule": "^2.1.0",
    "@types/react": "^18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typechain": "^8.0.0",
    "typescript": "^4.7.4",
    "yarn-upgrade-all": "^0.7.1"
  }
}
